{
  "manifest_version": 2,
  "name": "TabWrangler",
  "version": "1.0",
  "description": "Group and collapse tabs to reduce clutter in your workspace.",
  "permissions": ["tabs", "storage"],
  "background": {
    "scripts": ["background/background.js"],
    "page": "background/background-page.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "assets/icon.svg",
    "default_popup": ""
  },
  "icons": "assets/icon.svg"
}
